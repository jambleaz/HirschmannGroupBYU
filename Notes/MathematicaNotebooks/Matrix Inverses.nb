Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Mk", "[", 
    RowBox[{"d_", ",", "k_"}], "]"}], ":=", 
   RowBox[{
    FractionBox["1", 
     SuperscriptBox["2", 
      RowBox[{"k", "+", "1"}]]], 
    FractionBox["1", 
     SqrtBox[
      RowBox[{
       SuperscriptBox["d", "2"], "-", "4"}]]], 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"d", "+", 
         SqrtBox[
          RowBox[{
           SuperscriptBox["d", "2"], "-", "4"}]]}], ")"}], 
       RowBox[{"k", "+", "1"}]], "-", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"d", "-", 
         SqrtBox[
          RowBox[{
           SuperscriptBox["d", "2"], "-", "4"}]]}], ")"}], 
       RowBox[{"k", "+", "1"}]]}], ")"}]}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"c", "[", 
    RowBox[{"d_", ",", "k_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"i", ",", "j", ",", "cofactorMat"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"i", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"j", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"cofactorMat", "=", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "k"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"i", "=", "1"}], ",", 
        RowBox[{"i", "<=", "k"}], ",", 
        RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"j", "=", "1"}], ",", 
          RowBox[{"j", "<=", "k"}], ",", 
          RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"i", "<=", "j"}], ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"cofactorMat", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "j"}], "]"}], "]"}], "=", 
             RowBox[{
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"-", "1"}], ")"}], 
               RowBox[{"i", "+", "j"}]], "*", 
              RowBox[{"Mk", "[", 
               RowBox[{"d", ",", 
                RowBox[{"i", "-", "1"}]}], "]"}], "*", 
              RowBox[{"Mk", "[", 
               RowBox[{"d", ",", 
                RowBox[{"k", "-", "j"}]}], "]"}]}]}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"cofactorMat", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "j"}], "]"}], "]"}], "=", 
             RowBox[{
              SuperscriptBox[
               RowBox[{"(", 
                RowBox[{"-", "1"}], ")"}], 
               RowBox[{"j", "+", "i"}]], "*", 
              RowBox[{"Mk", "[", 
               RowBox[{"d", ",", 
                RowBox[{"j", "-", "1"}]}], "]"}], "*", 
              RowBox[{"Mk", "[", 
               RowBox[{"d", ",", 
                RowBox[{"k", "-", "i"}]}], "]"}]}]}]}], "\[IndentingNewLine]",
            "]"}]}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
