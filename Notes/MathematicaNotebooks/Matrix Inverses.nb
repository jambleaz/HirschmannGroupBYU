  RowBox[{"Array", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"triRows", "[", 
      RowBox[{"#1", ",", "#2", ",", "D"}], "]"}], "&"}], ",", 
    RowBox[{"{", 
     RowBox[{"n", ",", "n"}], "}"}]}], "]"}]}]}], "Input",
